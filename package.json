{
  "name": "mining-hr-position-qualification-ui",
  "version": "1.0.0",
  "description": "Mining Industry HR Position Qualification Definition Interface",
  "homepage": "/mining-hr",
  "main": "server.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "NODE_ENV=production node --max-old-space-size=16384 server.js",
    "dev": "react-scripts start",
    "prod": "npm run build && npm run server",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:restart": "pm2 restart HRAI-Mining-HR",
    "pm2:stop": "pm2 stop HRAI-Mining-HR",
    "pm2:delete": "pm2 delete HRAI-Mining-HR",
    "deploy:server": "git pull && npm install && npm run build && pm2 restart HRAI-Mining-HR",
    "logs": "pm2 logs HRAI-Mining-HR"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "mssql": "^12.0.0",
    "multer": "^2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "reactflow": "^11.11.4",
    "xlsx": "^0.18.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3050"
}
